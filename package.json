{
  "name": "react-alice-carousel",
  "version": "1.17.3",
  "description": "React image gallery, react slideshow carousel, react content rotator",
  "main": "./lib/react-alice-carousel",
  "types": "./lib/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmarinich/react-alice-carousel.git"
  },
  "scripts": {
    "build:dev": "webpack --colors --config config/webpack/webpack.config.js",
    "build:prod": "cross-env npm run clean && webpack --config config/webpack/webpack.lib.config.js --progress --profile --colors",
    "clean": "rm -rf lib dist",
    "remove:nm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "remove:dist": "npm run clean",
    "remove:lock": "find . -type f \\( -name 'yarn.lock' -or -name 'package-lock.json' \\) -delete",
    "remove:cache": "yarn cache clean --force; npm cache clean --force",
    "clear:all-hard": "npm-run-all 'remove:*'",
    "lib": "cross-env npm run clean && gulp --gulpfile config/gulp/gulpfile.js",
    "lint": "eslint ./src/**/**.js",
    "format": "prettier --write ./src/**/**.js",
    "postpublish": "git push --tags",
    "precommit": "cross-env npm run lint && npm test",
    "prepare": "npm run lib",
    "prepush": "cross-env npm run lint && npm test",
    "start": "webpack-dev-server --config config/webpack/webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "react",
    "carousel",
    "gallery",
    "slider",
    "slideshow",
    "rotator",
    "react-slideshow",
    "react-gallery",
    "react carousel",
    "react slideshow",
    "react gallery",
    "image gallery",
    "image slider",
    "image carousel",
    "image rotator",
    "content gallery",
    "content rotator"
  ],
  "author": "Max Marinich",
  "maintainers": [
    {
      "name": "Max Marinich",
      "email": "max.marinich@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxmarinich/react-alice-carousel/issues"
  },
  "homepage": "https://github.com/maxmarinich/react-alice-carousel#readme",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "src/lib/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "peerDependencies": {
    "react": ">=15.4"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "vanilla-swipe": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-hot-loader": "1.4.2",
    "css-loader": "1.0.0",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "5.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-strip-comments": "^2.5.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.15.0-rc.2",
    "jest": "22.1.0",
    "node-sass": "^4.13.0",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "^1.14.3",
    "react": ">=15.4",
    "react-addons-test-utils": "15.6.2",
    "react-dom": ">=15.4",
    "react-hot-loader": "4.3.5",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "16.4.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.22.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "1.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.11"
  }
}
